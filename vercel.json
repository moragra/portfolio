{
    "public": {
      "headers": [
        {
          "source": "/index.html",
          "headers": [
            {
              "key": "Content-Type",
              "value": "text/html"
            }
          ]
        },
        {
          "source": "/static/*",
          "headers": [
            {
              "key": "Content-Type",
              "value": "application/javascript"
            }
          ]
        }
      ]
    }
  }